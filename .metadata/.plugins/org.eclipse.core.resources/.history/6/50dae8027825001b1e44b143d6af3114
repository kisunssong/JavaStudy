//부모와 자식이 동일변수가 정의된 경우 super.변수명을 사용
class Father03 {
	int x = 10;
	int y = 20;
}
class Child03 extends Father03 {
	int x = 100;
	int y = 200;
	int z = 30;
	
	void print() {
		System.out.printf("x좌표:%d y좌표:%d z좌표:%d",super.x,super.y,z);
	}
}

public class WeekEx03 {
	public static void main(String[] args) {
		Child03 ch = new Child03();
		ch.print();
		//x좌표:100 y좌표:200 z좌표:30
		
		
	}
}
