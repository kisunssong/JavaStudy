import net.abc.model02.Circ;
import net.abc.model02.Rect;
import net.abc.model02.Tria;
import net.abc.model01.AbsRef;

public class Abs13 {
	static void goodsIn(Circ c) {
		c.draw();
	}
	static void goodsIn(Rect r) {
		r.draw();
	}
	static void goodsIn(Tria t) {
		t.draw();
	}
	static void goodsIn2(AbsRef abs) {
		abs.draw();
	}
	public static void main(String[] args) {
		Abs13.goodsIn(new Circ());//같은 클래스내에서는 정적메서드를
		//호출할 때 해당 클래스명을 생략해도 된다.
		goodsIn(new Rect());
		goodsIn(new Tria());
		
		System.out.println("======================");
		goodsIn2(new Circ());
		goodsIn2(new Rect());
		goodsIn2(new Tria());
		
		
	}
}
