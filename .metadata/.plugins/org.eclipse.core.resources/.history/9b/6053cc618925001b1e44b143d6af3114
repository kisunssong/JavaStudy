class Father07 {
	void f07() {
		System.out.println("조상클래스 메서드 f07()");
	}
}
class Child07 extends Father07 {
	void f07() {
		System.out.println("오버라이딩 자식 메서드 f07()");
	}
}

public class WeekEx07 {
	public static void main(String[] args) {
		Father07 f = new Father07();
		f.f07();
		
		Father07 f1 = new Child07();
		f1.f07();
	}
}
