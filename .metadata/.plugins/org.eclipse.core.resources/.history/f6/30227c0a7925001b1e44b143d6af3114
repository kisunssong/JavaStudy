//쥁젏 첔쫛첇 첊줦쩠쌰 촋첂왇 썭 super.줦쩠젌챹 牟
class Father03 {
	int x = 10;
	int y = 20;
}
class Child03 extends Father03 {
	int x = 100;
	int y = 200;
	int z = 30;
	
	void print() {
		System.out.printf("첔쫛 -> x촥킨:%d y촥킨:%d z촥킨:%d\n",x,y,z);
		System.out.printf("쥁젏 -> x촥킨:%d y촥킨:%d z촥킨:%d",super.x,super.y,z);
	}
}

public class WeekEx03 {
	public static void main(String[] args) {
		Child03 ch = new Child03();
		ch.print();
		//x촥킨:100 y촥킨:200 z촥킨:30
		
		
	}
}
