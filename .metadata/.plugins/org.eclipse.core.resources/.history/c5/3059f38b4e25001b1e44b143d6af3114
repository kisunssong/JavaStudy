
public class Date04 {
	public static void main(String[] args) {
		String s = "Hello";
		char c = s.charAt(1); 
		//첫문자 0부터 : e
		
		String n = "0123456";
		char c2 = n.charAt(2);
		//첫문자 0부터 : 2
		
		boolean result = s.equalsIgnoreCase("HELLO");
		//영문대솜누자를 굽누안함
		System.out.println(result);
		
		result = s.equals("HELLO");
		System.out.println(result);
	}
}
