import java.util.Vector;
import java.util.*;

public class ColGeneric03 {
	public static void main(String[] args) {

		Vector v = new Vector();
		v.add(10);
		v.add(20);
		v.add(30);
		for(int i=0; i<v.size(); i++) {
			System.out.println(v.get(i));
		}
		System.out.println("=================");
// 생성자 오버로딩, 오토박싱, 업캐스팅
		
		for(int i=1; i<=3; i++) {
			v.add(new Integer(i*10));
		}
		
	}
}


