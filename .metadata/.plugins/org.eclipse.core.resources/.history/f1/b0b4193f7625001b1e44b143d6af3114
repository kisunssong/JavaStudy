class Point01 { //ÃÖ°íÁ¶»ó Object
	private int x;
	private int y;
	
	public void setX(int x) {
		this.x = x;
	}
	public int getX() {
		return x;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getY() {
		return y;
	}
}
class Child01 extends Point01 {
	private int z;
	
	public void setZ(int z) {
		this.z = z;
	}
	public int getZ() {
		return z;
	}
}

public class WeekEx01 {
	public static void main(String[] args) {
		Child01 ch = new Child01();
		ch.setX(100);
		ch.setY(200);
		ch.setZ(1000000);
		
		System.out.printf("XÁÂÇ¥:%d YÁÂÇ¥:%d ZÁÂÇ¥:%d",ch.getX(),ch.getY(),ch.getZ());
	}
}
