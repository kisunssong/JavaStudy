class Father05 {
	void f05() {
		System.out.println("부모클래스 메서드 f05()");
	}
}
class Child05 extends Father05 {
	void f05() {
		System.out.println("자식클래스 메서드 f05()");
	}
}

public class WeekEx05 {
	public static void main(String[] args) {
		
		Father05 fa = new Child05(); //업캐스팅, 상속관계에서 자손->조상타입 변환
		fa.f05();

	}
}
