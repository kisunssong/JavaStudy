import net.abc.model01.AbsRef;
import net.abc.model02.Circ;
import net.abc.model02.Rect;
import net.abc.model02.Tria;


public class Abs12 {
	public static void main(String[] args) {
		
		Circ c = new Circ();
		c.draw();
		Rect r = new Rect();
		r.draw();
		Tria t = new Tria();
		t.draw();
		System.out.println("=====================");
		
		AbsRef ref;
		ref = new Circ();
		ref.draw();
		System.out.println(ref);
		ref = new Tria();
		ref.draw();
		System.out.println(ref);
		ref = new Rect();
		ref.draw();
		System.out.println(ref);
	}
}
